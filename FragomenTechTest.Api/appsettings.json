{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "RateLimit": {
    "Window": 1,
    "PermitLimit": 1,
    "QueueLimit": 2
  },
  "OpenWeatherMap": {
    "ApiKey": "[[API_KEY]]",
    "BaseUrl": "https://api.openweathermap.org/"
  },
  "WeatherBit": {
    "ApiKey": "[[API_KEY]]",
    "BaseUrl": "https://api.weatherbit.io/"
  },
  "Caching": {
    "CacheEnabled": true,
    "CacheSlidingExpiration": "0:10:0",
    "ChangesPollingInterval": "0:0:15"
  },
  "AllowedHosts": "*"
}
